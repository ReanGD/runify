{
  "logger": {
    "addcallerinfo": false,
    "compress": true,
    "enablerotate": true,
    "format": "plain",
    "level": "debug",
    "levelstacktrace": "error",
    "localtimeinbackupfilename": true,
    "maxagedays": 3,
    "maxbackups": 0,
    "maxsizemb": 100,
    "output": "runify.log"
  },
  "shortcuts": {
    "root": "Super+R"
  },
  "system": {
    "RpcAddress": "/tmp/runify.socket",
    "Terminal": "sh",
    "UIBinaryPath": "../ui/build/linux/x64/debug/bundle/runify-ui"
  }
}
